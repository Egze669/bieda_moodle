{% extends 'base.html.twig' %}

{% block title %}Hello StudentController!{% endblock %}

{% block body %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    <div class="example-wrapper">
        <ul>
        {% for teachers in teachersArray %}
            <li><a href="{{  url('teacher_tasks',{'email': teachers['email']}) }}"> {{ teachers.name }} {{ teachers.surname }} </a></li>
        {% endfor %}
        </ul>
    </div>
{% endblock %}
